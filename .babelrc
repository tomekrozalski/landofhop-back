{
	"presets": [
		["@babel/env", {
			"targets": {
				"node": "current"
			}
		}]
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		["module-resolver", {
			"root": ["./src/v1"],
			"alias": {
				"utils": "./src/v1/utils"
			}
		}]
	]
}
